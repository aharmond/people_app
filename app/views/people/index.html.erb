<h1> People </h1>
<p><%= link_to 'Add Person', new_person_path %></p>
<% @people.each do |person| %>
  <h2><%= person.full_name %></h2>
  <%= link_to 'Go to Person', person_path(person) %>
  <%= link_to 'Edit Person', edit_person_path(person) %>
  <%= link_to 'Remove Person', person, method: :delete %>
<% end %>
<p>
<%= link_to 'Return Home', root_path %>
</p>