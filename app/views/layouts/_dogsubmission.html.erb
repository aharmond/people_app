<%= form_for @dog do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :age %>
  <%= f.text_field :age %>

  <%= f.label :breed %>
  <%= f.text_field :breed %>

  <%= f.label :color %>
  <%= f.select :color, ['Brown', 'Black', 'Red', 'Yellow', 'Cream', 'Fawn', 'Blue', 'Grey', 'White', 'Other'] %>

  <%= f.submit %>
<% end %>